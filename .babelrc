{
  "presets": [
    [
      "@1stg",
      {
        "react": true,
        "isTSX": true
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".scss"
        ],
        "root": [
          "./src"
        ]
      }
    ]
  ]
}
